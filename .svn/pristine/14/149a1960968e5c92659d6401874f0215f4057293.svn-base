<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kkl.kklplus.provider.rpt.common.mapper.SysUserSubMapper">

    <select id="getSubUserIdsOfManager" resultType="java.lang.Long">
        SELECT
            sub_user_id
        FROM
            sys_user_sub_1
        where
            user_id = #{userId}
        LIMIT 5000
    </select>

</mapper>