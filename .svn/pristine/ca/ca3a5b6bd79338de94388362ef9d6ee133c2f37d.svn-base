<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kkl.kklplus.provider.rpt.mapper.FinancialReviewDetailsRptMapper">

    <select id="getFinancialReviewDetailsList" resultType="com.kkl.kklplus.entity.rpt.RPTFinancialReviewDetailsEntity">
        SELECT
        order_no, create_at AS "createDt", kefu_id, reason, sub_Type
        FROM sd_abnormal_form
        WHERE
        form_type=5
        and create_at >= #{startDate}
        and <![CDATA[ create_at <= #{endDate} ]]>
    </select>

    <select id="getFinancialReviewDetailsByList" resultType="com.kkl.kklplus.entity.rpt.RPTFinancialReviewDetailsEntity">
        SELECT
        order_no, create_at AS "createDt", kefu_id, reason, sub_Type
        FROM sd_abnormal_form
        WHERE
        form_type=5
        and create_at >= #{startDate}
        and <![CDATA[ create_at <= #{endDate} ]]>
    </select>


    <select id="hasReportData" resultType="java.lang.Integer">
        SELECT
        count(0)
        FROM sd_abnormal_form
        WHERE
        form_type=5
        and create_at >= #{startDate}
        and <![CDATA[ create_at <= #{endDate} ]]>
    </select>

</mapper>